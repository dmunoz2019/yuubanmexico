<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <template id="report_lot_images">
        <t t-call="web.html_container">
            <t t-foreach="docs" t-as="doc">
                <div class="page">
                    <div class="row">
                        <div class="col-auto">
                            <img t-attf-src="data:image/*;base64,{{doc.datas}}" style="width:950px; margin-left:-90px;height:800px;margin-top:-200px;" />
                        </div>
                    </div>
                </div>
            </t>
        </t>
    </template>

</odoo>